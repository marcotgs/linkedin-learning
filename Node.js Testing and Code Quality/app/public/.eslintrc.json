{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": false,
		"jquery": true,
		"browser": true
	},
	"extends": "eslint:recommended",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
