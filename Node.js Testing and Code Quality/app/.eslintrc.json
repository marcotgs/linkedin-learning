{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true,
		"jquery": false,
		"browser": false
	},
	"extends": "eslint:recommended",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"no-empty": "error",
		"prefer-arrow-callback": "error",
		"prefer-const": "error",
		"no-var": "error",
		"no-multiple-empty-lines": "warn"
	}
}
